<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RestControllerService WSDL service -->

  <service name="RestController" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://controller"/>
      <parameter name="wsdlServiceElement" value="RestControllerService"/>
      <parameter name="schemaQualified" value="http://model,http://controller"/>
      <parameter name="wsdlServicePort" value="RestController"/>
      <parameter name="className" value="controller.RestControllerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="RestController"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addBook" qname="operNS:addBook" xmlns:operNS="http://controller" returnQName="retNS:addBookReturn" xmlns:retNS="http://controller" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:book" xmlns:pns="http://controller" type="tns:Book" xmlns:tns="http://model"/>
      </operation>
      <operation name="getBooks" qname="operNS:getBooks" xmlns:operNS="http://controller" returnQName="retNS:getBooksReturn" xmlns:retNS="http://controller" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://controller" returnItemQName="tns:item" xmlns:tns="http://controller" soapAction="" >
      </operation>
      <operation name="updateBook" qname="operNS:updateBook" xmlns:operNS="http://controller" returnQName="retNS:updateBookReturn" xmlns:retNS="http://controller" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:book" xmlns:pns="http://controller" type="tns:Book" xmlns:tns="http://model"/>
      </operation>
      <operation name="getBooksByTitle" qname="operNS:getBooksByTitle" xmlns:operNS="http://controller" returnQName="retNS:getBooksByTitleReturn" xmlns:retNS="http://controller" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://controller" returnItemQName="tns:item" xmlns:tns="http://controller" soapAction="" >
        <parameter qname="pns:title" xmlns:pns="http://controller" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deleteBooks" qname="operNS:deleteBooks" xmlns:operNS="http://controller" returnQName="retNS:deleteBooksReturn" xmlns:retNS="http://controller" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="deleteBookById" qname="operNS:deleteBookById" xmlns:operNS="http://controller" returnQName="retNS:deleteBookByIdReturn" xmlns:retNS="http://controller" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://controller" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getBooksByTitle deleteBookById getBooks updateBook deleteBooks addBook"/>

      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>getBooks"
        type="java:controller.GetBooks"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>updateBookResponse"
        type="java:controller.UpdateBookResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>getBooksByTitle"
        type="java:controller.GetBooksByTitle"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>deleteBookById"
        type="java:controller.DeleteBookById"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>deleteBooksResponse"
        type="java:controller.DeleteBooksResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://controller"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://model"
        qname="ns:Book"
        type="java:model.Book"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>deleteBooks"
        type="java:controller.DeleteBooks"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>addBookResponse"
        type="java:controller.AddBookResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>getBooksResponse"
        type="java:controller.GetBooksResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>deleteBookByIdResponse"
        type="java:controller.DeleteBookByIdResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>updateBook"
        type="java:controller.UpdateBook"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>addBook"
        type="java:controller.AddBook"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller"
        qname="ns:>getBooksByTitleResponse"
        type="java:controller.GetBooksByTitleResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
